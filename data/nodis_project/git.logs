+        )
+    estimated_volume = sum(float(est.total_volume) for est in estimated_jobs)
+    if completed_volume > estimated_volume:
+        return (1, completed_volume - estimated_volume)
+    return (completed_volume / estimated_volume if estimated_volume > 0 else 0, 0)
