@@ -0,0 +1,72 @@
+"use client"
+
+import { useEffect, useState } from "react"
+import { IconButton } from "@chakra-ui/react"
+import { BsThreeDotsVertical } from "react-icons/bs"
+import { MenuRoot, MenuTrigger, MenuContent } from "@/components/ui/menu"
+import useAuth from "@/hooks/useAuth"
+import EditCounterparty from "./EditCounterparty"
+import DeleteCounterparty from "./DeleteCounterparty"
+import ReinstateCounterparty from "./ReinstateCounterparty"
+import DestroyCounterparty from "./DestroyCounterparty"
+import { type CounterpartyPublic } from "@/client"
+
+interface CounterpartyActionsMenuProps {
+  counterparty: CounterpartyPublic
+  onUpdate?: (updated: CounterpartyPublic) => void
+}
+
+export const CounterpartyActionsMenu = ({
+  counterparty,
+  onUpdate,
+}: CounterpartyActionsMenuProps) => {
+  const { user } = useAuth()
+  const [isActive, setIsActive] = useState(counterparty.is_active)
+
+  useEffect(() => setIsActive(counterparty.is_active), [counterparty.is_active])
+
+  const handleDelete = () => {
+    setIsActive(false)
+    onUpdate?.({ ...counterparty, is_active: false })
+  }
+
+  const handleReinstate = () => {
+    setIsActive(true)
+    onUpdate?.({ ...counterparty, is_active: true })
+  }
+
+  const handleEditSuccess = (updated: any) => {
+    onUpdate?.(updated)
+  }
+
+  return (
+    <MenuRoot>
+      <MenuTrigger asChild>
+        <IconButton variant="ghost" color="inherit" size="sm">
+          <BsThreeDotsVertical />
+        </IconButton>
+      </MenuTrigger>
+
+      <MenuContent>
+        <EditCounterparty
+          counterparty={counterparty}
+          onSuccess={handleEditSuccess}
+        />
+        {isActive ? (
+          <DeleteCounterparty
+            counterpartyId={counterparty.id}
+            onSuccess={handleDelete}
+          />
+        ) : (
+          <ReinstateCounterparty
+            counterpartyId={counterparty.id}
+            onSuccess={handleReinstate}
+          />
+        )}
+        {user?.is_superuser && (
+          <DestroyCounterparty counterpartyId={counterparty.id} />
+        )}
+      </MenuContent>
+    </MenuRoot>
+  )
+}
