--- /dev/null
+++ b/frontend/src/components/Projects/JobsTable.tsx
@@ -0,0 +1,443 @@
+import { useState, useMemo } from 'react';
+import {
+  Table,
+  Box,
+  HStack,
+  VStack,
+  Text,
+  Button,
+  Input,
+  NativeSelect,
+  Badge,
+  Menu,
+  IconButton,
+  Collapsible,
+} from '@chakra-ui/react';
+import { FiTrash2, FiEdit, FiChevronDown, FiChevronUp, FiPlus } from 'react-icons/fi';
+import { ProgressService, JobsTypePublic, JobPublic, JobCreate, JobUpdate, ProjectPublicDetailed } from '@/client';
+
+interface JobsTableProps {
+  projectId: ProjectPublicDetailed;
+  jobsDoned: JobPublic[];
+}
+
+export function JobsTable({ projectId, jobsDoned }: JobsTableProps) {
+  const [jobs_doned, setJobs] = useState<JobPublic[]>([]); 
+  const [expandedDates, setExpandedDates] = useState<Set<string>>(new Set());
+  const [editingJob, setEditingJob] = useState<string | null>(null);
+  const [editForm, setEditForm] = useState<Partial<JobUpdate>>({});
+
+  // New job form state
+  const [newJob, setNewJob] = useState<Omit<JobCreate, 'project_id' | 'report_id'>>({
+    day: new Date().toISOString().split('T')[0], // Today's date
+    volume: '',
+    jobs_type_id: '',
+  });
+
+  // Group jobs by date
+  const jobsByDate = useMemo(() => {
+    const grouped: { [date: string]: JobPublic[] } = {};
+    
+    jobsDoned.forEach(job => {
+      if (!grouped[job.day]) {
+        grouped[job.day] = [];
+      }
+      grouped[job.day].push(job);
+    });
+    
+    // Sort dates descending (newest first)
+    return Object.entries(grouped)
+      .sort(([dateA], [dateB]) => new Date(dateB).getTime() - new Date(dateA).getTime())
+      .reduce((acc, [date, jobs]) => {
+        acc[date] = jobs.sort((a, b) => a.jobs_type_id.localeCompare(b.jobs_type_id));
+        return acc;
+      }, {} as { [date: string]: JobPublic[] });
+  }, [jobsDoned]);
+
+  // Toggle date group expansion
+  const toggleDate = (date: string) => {
+    setExpandedDates(prev => {
+      const newSet = new Set(prev);
+      if (newSet.has(date)) {
+        newSet.delete(date);
+      } else {
+        newSet.add(date);
+      }
+      return newSet;
+    });
+  };
+
+  // Format date for display
+  const formatDate = (dateString: string) => {
+    const date = new Date(dateString);
+    return date.toLocaleDateString('ru-RU', {
+      weekday: 'short',
+      year: 'numeric',
+      month: 'short',
+      day: 'numeric'
+    });
+  };
+
+  // Calculate total volume for a date group
+  const getDateTotalVolume = (jobs: JobPublic[]) => {
+    return jobs.reduce((total, job) => total + parseFloat(job.volume), 0);
+  };
+
+  // Add new job
+  const handleAddJob = async () => {
+    if (!newJob.jobs_type_id || !newJob.volume) return;
+
+    try {
+      const response = await ProgressService.createJobEndpoint({
+        requestBody: {
+          ...newJob,
+          volume: Number(newJob.volume),
+          project_id: projectId,
+          report_id: null,
+        } as JobCreate,
+      });
+
+      setJobs(prev => prev.map(job => job.id === jobId ? response : job));
+      setNewJob({
+        day: new Date().toISOString().split('T')[0],
+        volume: '',
+        jobs_type_id: '',
+      });
+      
+      // Expand the date group for the new job
+      setExpandedDates(prev => new Set(prev).add(newJob.day));
+    } catch (error) {
+      console.error('Failed to create job:', error);
+    }
+  };
+
+  // Start editing a job
+  const handleEdit = (job: JobPublic) => {
+    setEditingJob(job.id);
+    setEditForm({
+      day: job.day,
+      volume: job.volume,
+      jobs_type_id: job.jobs_type_id,
+    });
+  };
+
+  // Save edited job
+  const handleSaveEdit = async (jobId: string) => {
+    try {
+      const response = await ProgressService.updateJobEndpoint({
+        jobId,
+        requestBody: {
+          ...editForm,
+          volume: editForm.volume ? Number(editForm.volume) : undefined,
+        } as JobUpdate,
+      });
+
+      setJobs(prev => prev.map(job => job.id === jobId ? response : job));
+      setEditingJob(null);
+      setEditForm({});
+    } catch (error) {
+      console.error('Failed to update job:', error);
+    }
+  };
+
+  // Cancel editing
+  const handleCancelEdit = () => {
+    setEditingJob(null);
+    setEditForm({});
+  };
+
+  // Delete job
+  const handleDelete = async (jobId: string) => {
+    try {
+      await ProgressService.deleteJobEndpoint({ jobId });
+      setJobs(prev => prev.filter(job => job.id !== jobId));
+    } catch (error) {
+      console.error('Failed to delete job:', error);
+    }
+  };
+
+  // Get jobs type title by ID
+  const getJobsTypeTitle = (jobsTypeId: string) => {
+    return projectId.jobs_types?.find(jt => jt.id === jobsTypeId)?.title || '‚Äî';
+  };
+
