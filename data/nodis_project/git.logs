+from fastapi import APIRouter, Depends, HTTPException
+from sqlmodel import Session
+from app.api.deps import SessionDep
+from app.models import Worker, WorkerCreate, WorkerUpdate, WorkerPublic, WorkersPublic
+from app.crud import create_worker, get_worker, get_workers, update_worker, delete_worker
+
+router = APIRouter(prefix="/workers", tags=["users"])
+
+@router.post("/", response_model=WorkerPublic)
+def create_worker_endpoint(worker_in: WorkerCreate, session: SessionDep):
+    return create_worker(session, worker_in)
+
+@router.get("/{worker_id}", response_model=WorkerPublic)
+def read_worker(worker_id: int, session: SessionDep):
+    worker = get_worker(session, worker_id)
+    if not worker:
+        raise HTTPException(status_code=404, detail="Worker not found")
+    return worker
+
+@router.get("/", response_model=WorkersPublic)
+def read_workers(session: SessionDep, skip: int = 0, limit: int = 100):
+    workers = get_workers(session, skip=skip, limit=limit)
+    workers_public = [WorkerPublic.model_validate(w) for w in workers]
+    return WorkersPublic(data=workers_public, count=len(workers_public))
+
+@router.put("/{worker_id}", response_model=WorkerPublic)
+def update_worker_endpoint(worker_id: int, worker_in: WorkerUpdate, session: SessionDep):
+    worker = get_worker(session, worker_id)
+    if not worker:
+        raise HTTPException(status_code=404, detail="Worker not found")
+    return update_worker(session, worker, worker_in)
+
+@router.delete("/{worker_id}")
+def delete_worker_endpoint(worker_id: int, session: SessionDep):
+    worker = get_worker(session, worker_id)
+    if not worker:
+        raise HTTPException(status_code=404, detail="Worker not found")
+    delete_worker(session, worker_id)
+    return {"ok": True}
