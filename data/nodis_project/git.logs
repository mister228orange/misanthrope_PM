+async def upload_jobs_type_category_tree(file: UploadFile, session: SessionDep):
+    """
+    Upload an XML file containing a category tree.
+    The XML will be saved as text, parsed to JSON, and validated.
+    """
+    try:
+        # Read file content
+        content = await file.read()
+        text = content.decode("utf-8")
+
+        # Attempt to parse XML
+        try:
+            parsed = text
+            valid = True
+        except Exception as e:
+            parsed = None
+            valid = False
+
+        # Save to DB
+        tree = JobsTypeCategoryTree(
+            user_xml=text,
+            tree=json.dumps(parsed),
+            is_valid=valid
+        )
+
+        session.add(tree)
+        session.commit()
+        session.refresh(tree)
+        return tree
+
+    except Exception as e:
+        raise HTTPException(status_code=400, detail=f"Failed to upload XML: {e}")
+
