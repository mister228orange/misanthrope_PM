+import { useEffect, useState } from "react"
+import { IconButton } from "@chakra-ui/react"
+import { BsThreeDotsVertical } from "react-icons/bs"
+import { MenuRoot, MenuTrigger, MenuContent } from "@/components/ui/menu"
+import useAuth from "@/hooks/useAuth"
+import EditBuildObject from "./EditBuildObject"
+import DeleteBuildObject from "./DeleteBuildObject"
+import ReinstateBuildObject from "./ReinstateBuildObject"
+import DestroyBuildObject from "./DestroyBuildObject"
+import { type BuildObjectPublic } from "@/client"
+
+interface BuildObjectActionsMenuProps {
+  buildObject: BuildObjectPublic
+  onUpdate?: (updated: BuildObjectPublic) => void
+}
+
+export const BuildObjectActionsMenu = ({
+  buildObject,
+  onUpdate,
+}: BuildObjectActionsMenuProps) => {
+  const { user } = useAuth()
+  const [isActive, setIsActive] = useState(buildObject.is_active)
+
+  useEffect(() => {
+    setIsActive(buildObject.is_active)
+  }, [buildObject.is_active])
+
+  const handleDelete = () => {
+    setIsActive(false)
+    onUpdate?.({ ...buildObject, is_active: false })
+  }
+
+  const handleReinstate = () => {
+    setIsActive(true)
+    onUpdate?.({ ...buildObject, is_active: true })
+  }
+
+  const handleEditSuccess = (updated: BuildObjectPublic) => {
+    onUpdate?.(updated)
+  }
+
+  return (
+    <MenuRoot>
+      <MenuTrigger asChild>
+        <IconButton variant="ghost" color="inherit" size="sm">
+          <BsThreeDotsVertical />
+        </IconButton>
+      </MenuTrigger>
+
+      <MenuContent>
+        <EditBuildObject
+          buildObject={buildObject}
+          onSuccess={handleEditSuccess}
+        />
+        {isActive ? (
+          <DeleteBuildObject
+            buildObjectId={buildObject.id}
+            onSuccess={handleDelete}
+          />
+        ) : (
+          <ReinstateBuildObject
+            buildObjectId={buildObject.id}
+            onSuccess={handleReinstate}
+          />
+        )}
+        {user?.is_superuser && (
+          <DestroyBuildObject buildObjectId={buildObject.id} />
+        )}
+      </MenuContent>
+    </MenuRoot>
+  )
+}
