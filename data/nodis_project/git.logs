+    jobs = session.exec(select(Job).where(and_(Job.project_id == project_id, Job.jobs_type_id == jt_id))) # type: ignore
+    completed_volume = sum(float(j.volume) for j in jobs)
+    estimated_jobs = session.exec(select(JobsEstimation).where(
+        and_(
+            JobsEstimation.project_id == project_id, 
+            JobsEstimation.jobs_type_id == jt_id)
