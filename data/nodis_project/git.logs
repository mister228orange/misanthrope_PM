+import { Button } from "@chakra-ui/react"
+import { useMutation, useQueryClient, UseMutationResult } from "@tanstack/react-query"
+import { FaUndo } from "react-icons/fa"
+
+import {
+  NomenclatureService,
+  type NomenclatureReinstateMaterialTypeResponse,
+  type NomenclatureReinstateMaterialTypeData,
+} from "@/client"
+import useCustomToast from "@/hooks/useCustomToast"
+import { handleError } from "@/utils"
+import { ApiError } from "@/client/core/ApiError"
+
+interface ReinstateMaterialTypeProps {
+  materialTypeId: string
+  onSuccess?: () => void
+}
+
+export default function ReinstateMaterialType({ materialTypeId, onSuccess }: ReinstateMaterialTypeProps) {
+  const queryClient = useQueryClient()
+  const { showSuccessToast } = useCustomToast()
+
+  const mutation: UseMutationResult<
+    NomenclatureReinstateMaterialTypeResponse,
+    ApiError,
+    NomenclatureReinstateMaterialTypeData
+  > = useMutation({
+    mutationFn: (data) => NomenclatureService.reinstateMaterialType(data),
+    onSuccess: () => {
+      showSuccessToast("–¢–∏–ø –º–∞—Ç–µ—Ä–∏–∞–ª–∞ —É—Å–ø–µ—à–Ω–æ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω.")
+      queryClient.invalidateQueries({ queryKey: ["materialTypes"] })
+      onSuccess?.()
+    },
+    onError: (err) => {
+      handleError(err as ApiError)
+    },
+  })
+
+  return (
+    <Button
+      variant="ghost"
+      size="sm"
+      colorPalette="green"
+      type="button"
+      onClick={() => mutation.mutate({ materialTypeId })}
+      loading={mutation.isPending}
+    >
+      <FaUndo /> –í–æ—Å—Å—Ç–∞–Ω–æ–≤–∏—Ç—å
+    </Button>
+  )
+}
