+def compute_progress(project: Project, session: Session):
+        jobs_types = session.exec(select(JobsEstimation.jobs_type_id).where(JobsEstimation.project_id == project.id)).all()
+        current_total_progress, total_overhead = 0, {}
+
+        for jt in jobs_types:
+            complete_part, overhead  = compute_progress_by_jobs_type(session, jt, project.id)
+            
+            current_total_progress += complete_part / len(jobs_types)
+            total_overhead[jt] = overhead 
+
