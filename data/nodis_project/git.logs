@@ -0,0 +1,73 @@
+from fastapi import APIRouter, Depends, HTTPException
+from sqlmodel import Session
+from app.api.deps import SessionDep, get_current_user
+from .models import (
+    Inventory, InventoryCreate, InventoryUpdate, InventoryPublic,
+    InventoryType, InventoryTypeCreate, InventoryTypeUpdate, InventoryTypePublic,
+    InventoryAction, InventoryActionCreate, InventoryActionPublic
+)
+from .crud import (
+    create_inventory, get_inventory, get_inventories, update_inventory, delete_inventory,
+    create_inventory_type, get_inventory_types, update_inventory_type,
+    create_inventory_action
+)
+
+router = APIRouter(prefix="/inventory", tags=["inventory"])
+
+
+# -------- INVENTORY --------
+@router.post("/", response_model=InventoryPublic, dependencies=[Depends(get_current_user)])
+def create_inventory_endpoint(inv_in: InventoryCreate, session: SessionDep):
+    return create_inventory(session, inv_in)
+
+
+@router.get("/{inv_id}", response_model=InventoryPublic, dependencies=[Depends(get_current_user)])
+def read_inventory(inv_id: str, session: SessionDep):
+    inv = get_inventory(session, inv_id)
+    if not inv:
+        raise HTTPException(status_code=404, detail="Inventory not found")
+    return inv
+
+
+@router.get("/", response_model=list[InventoryPublic], dependencies=[Depends(get_current_user)])
+def read_inventories(session: SessionDep, skip=0, limit=100):
+    return get_inventories(session, skip=skip, limit=limit)
+
+
+@router.put("/{inv_id}", response_model=InventoryPublic, dependencies=[Depends(get_current_user)])
+def update_inventory_endpoint(inv_id: str, inv_in: InventoryUpdate, session: SessionDep):
+    inv = get_inventory(session, inv_id)
+    if not inv:
+        raise HTTPException(status_code=404, detail="Inventory not found")
+    return update_inventory(session, inv, inv_in)
+
+
+@router.delete("/{inv_id}", dependencies=[Depends(get_current_user)])
+def delete_inventory_endpoint(inv_id: str, session: SessionDep):
+    delete_inventory(session, inv_id)
+    return {"ok": True}
+
+
+# -------- INVENTORY TYPE --------
+@router.post("/types/", response_model=InventoryTypePublic, dependencies=[Depends(get_current_user)])
+def create_inventory_type_endpoint(type_in: InventoryTypeCreate, session: SessionDep):
+    return create_inventory_type(session, type_in)
+
+
+@router.get("/types/", response_model=list[InventoryTypePublic], dependencies=[Depends(get_current_user)])
+def read_inventory_types(session: SessionDep):
+    return get_inventory_types(session)
+
+
+@router.put("/types/{type_id}", response_model=InventoryTypePublic, dependencies=[Depends(get_current_user)])
+def update_inventory_type_endpoint(type_id: int, type_in: InventoryTypeUpdate, session: SessionDep):
+    db_type = session.get(InventoryType, type_id)
+    if not db_type:
+        raise HTTPException(status_code=404, detail="Inventory type not found")
+    return update_inventory_type(session, db_type, type_in)
+
+
+# -------- INVENTORY ACTIONS --------
+@router.post("/actions/", response_model=InventoryActionPublic, dependencies=[Depends(get_current_user)])
+def create_inventory_action_endpoint(action_in: InventoryActionCreate, session: SessionDep):
+    return create_inventory_action(session, action_in)
