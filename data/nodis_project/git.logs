+import { Box, Text, Flex, Progress } from "@chakra-ui/react"
+import { FC } from "react"
+
+interface ProjectCardProps {
+  title: string
+  status: string
+  progress: number
+  onClick?: () => void
+}
+
+export const ProjectCard: FC<ProjectCardProps> = ({
+  title,
+  status,
+  progress,
+}) => {
+  const borderColor =
+    status === "active" ? "blue.400" : status === "closed" ? "gray.400" : "gray.300"
+  const shadow = status === "active" ? "md" : "none"
+
+  return (
+    <Box
+      w="180px"
+      h="110px"
+      borderWidth="3px"
+      borderColor={borderColor}
+      borderRadius="md"
+      boxShadow={shadow}
+      p={3}
+      cursor="pointer"
+      bg="white"
+      _hover={{ transform: "scale(1.03)" }}
+      transition="all 0.2s"
+      position="relative"
+    >
+      <Flex justify="center" align="center" h="full">
+        <Text fontWeight="medium" fontSize="sm" textAlign="center">
+          {title}
+        </Text>
+      </Flex>
+
+      {/* external bottom border progress */}
+      <Box
+        position="absolute"
+        left="-3px"
+        right="-3px"
+        bottom="-3px"
+        borderBottomRadius="md"
+      >
+        <Progress.Root value={progress} size="xs" colorPalette="green" w="full">
+          <Progress.Track>
+            <Progress.Range />
+          </Progress.Track>
+        </Progress.Root>
+      </Box>
+    </Box>
+  )
+}
