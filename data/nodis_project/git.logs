+
+
+def bounty_disactualizate(job_state: JobBountyState, session: Session):
+    bounty_volume = compute_bounty_state(job_state, session)
+    if bounty_volume:
+        workers_team = get_workers_team(job_state.workers_ids)
+        for worker in workers_team:
+            current_worker_salary = float(worker.current_mined_salary)
+            worker.current_mined_salary = current_worker_salary - bounty_volume * float(worker.productivity_rate) / len(workers_team)  # type: ignore
+            session.add(worker)
