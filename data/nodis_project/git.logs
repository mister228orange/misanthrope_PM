+
+function getReports() {
+  return {
+    queryFn: () => ProgressService.readReports({ skip: 0, limit: 1000 }),
+    queryKey: ["reports"],
+  }
+}
+
+export const Route = createFileRoute("/_layout/reports")({
+  component: ReportsPage,
+})
+
+function LoadingState() {
+  return (
+    <Flex justify="center" align="center" py={12}>
+      <Spinner size="lg" />
+    </Flex>
+  )
+}
+
+function ErrorState({ error }: { error: any }) {
+  return (
+    <Flex justify="center" align="center" py={12}>
+      <Text color="red.500">Error loading reports: {error?.message || 'Unknown error'}</Text>
+    </Flex>
+  )
+}
+
+function ReportsPage() {
+  const { data, isLoading, error } = useQuery(getReports())
+  const [selectedReport, setSelectedReport] = useState<ReportPublic | null>(null)
