+  Flex,
+  Avatar,
+} from "@chakra-ui/react";
+import { FC, useState } from "react";
+import { ProgressService, ReportPublic } from "@/client";
+import { JobCreate, JobsTypePublic } from "@/client";
+import { getUserId } from "@/hooks/useAuth";
+import { JobsTypeSelectStandalone, ProjectSelect, WorkerMultiSelect } from "../Common/Selects";
+import useCustomToast from "@/hooks/useCustomToast";
+import { FiTrash2 } from "react-icons/fi";
+
+interface HandleNewWorkerReportProps {
+  report: ReportPublic;
+  onClose: () => void;
+  onAccepted?: (reportId: string) => void;
+  onDeclined?: (reportId: string) => void;
+}
+
+interface NewJobRow {
+  stage: string;
+  jobs_type: JobsTypePublic | null;
+  total_volume?: number;
+}
+
+export const HandleNewWorkerReport: FC<HandleNewWorkerReportProps> = ({
+  report,
+  onClose,
+  onAccepted,
+  onDeclined,
+}) => {
+  const [selectedProjectId, setSelectedProjectId] = useState("");
+//   const [selectedWorkers, setSelectedWorkers] = useState<string[]>([
+//     report.sender_tg_id.toString(),
+//   ]);
+
+  const [currentAddon, setCurrentAddon] = useState("");
+  const userId = getUserId();
+  const [jobs, setJobs] = useState<NewJobRow[]>([]);
+  const [isLoading, setIsLoading] = useState(false);
+  const [selectedWorkers, setSelectedWorkers] = useState<string[]>([])
+
+  const [newJob, setNewJob] = useState<NewJobRow>({
+    stage: "",
+    jobs_type: null,
+    total_volume: undefined,
+  });
+
+  const { showSuccessToast, showErrorToast } = useCustomToast();
+
+  const handleAddJobRow = () => {
+    if (!newJob.jobs_type || !newJob.total_volume) {
+      showErrorToast("Please fill in job type and volume");
+      return;
+    }
+
+    setJobs((prev) => [...prev, { ...newJob }]);
+
+    setNewJob({
+      stage: "",
+      jobs_type: null,
+      total_volume: undefined,
+    });
+    setCurrentAddon("");
+  };
+
+  const handleRemoveJob = (index: number) => {
+    setJobs((prev) => prev.filter((_, i) => i !== index));
+  };
+
+  const handleNewJobChange = (field: keyof NewJobRow, value: any) => {
+    setNewJob((prev) => ({ ...prev, [field]: value }));
+  };
+
+  const handleJobsTypeChange = (jobsType: JobsTypePublic | null) => {
+    handleNewJobChange("jobs_type", jobsType);
+
+    if (jobsType?.measurement_unit) setCurrentAddon(jobsType.measurement_unit);
+    else setCurrentAddon("");
+  };
+
+  const handleAcceptReport = async () => {
+    if (!selectedProjectId || jobs.length === 0) {
+      showErrorToast("Please select a project and add at least one job");
+      return;
+    }
+
+    setIsLoading(true);
+    try {
+      const jobsReported: JobCreate[] = jobs.map((job) => ({
+        day: new Date().toISOString().split("T")[0],
+        volume: job.total_volume || 0,
+        jobs_type_id: job.jobs_type?.id ?? "",
+        project_id: selectedProjectId,
+        workers_ids: "1,2",//selectedWorkers.join(","),
+        report_id: report.id,
+      }));
+
+      const requestBody = {
+        simple_report: {
+          id: report.id,
+          reviewer_id: userId || "",
+          project_id: selectedProjectId,
+        },
+        jobs_reported: jobsReported,
+      };
+
+      await ProgressService.acceptSimpleReport({ requestBody });
+
+      showSuccessToast("Report accepted successfully");
+
+      onAccepted?.(report.id);
+      onClose();
+    } catch (error) {
+      console.error(error);
+      showErrorToast("Failed to accept report");
+    } finally {
+      setIsLoading(false);
+    }
+  };
+
+  const handleDeclineReport = async () => {
+    setIsLoading(true);
+    try {
+      await ProgressService.declineSimpleReport({
+        simpleReportId: report.id,
+      });
+
+      showSuccessToast("Report declined");
+      onDeclined?.(report.id);
+      onClose();
+    } catch (error) {
+      console.error(error);
+      showErrorToast("Failed to decline report");
+    } finally {
+      setIsLoading(false);
+    }
+  };
+
+  return (
+    <Dialog.Root open onOpenChange={(d) => !d.open && onClose()}>
+      <Portal>
+        <Dialog.Backdrop />
+        <Dialog.Positioner>
+          <Dialog.Content width="95vw" maxW="1600px" p={0}>
+            <Flex height="85vh" overflow="hidden">
+              {/* LEFT COLUMN */}
+              <Card.Root width="40%" borderRight="1px solid #eee" borderRadius={0}>
+                <CardHeader borderBottom="1px solid #eee" pb={4}>
+                  <Flex gap={3} align="center">
+                    <Avatar.Root>
+                        <Avatar.Image src="https://images.unsplash.com/photo-1511806754518-53bada35f930" />
+                        <Avatar.Fallback name={report.sender_tg_id.toString()} />
+                    </Avatar.Root>
+                    <Box>
+                      <Text fontSize="lg" fontWeight="bold">
+                        Worker Report
+                      </Text>
+                      <Text fontSize="sm" color="gray.600">
+                        TG User: {report.sender_tg_id}
+                      </Text>
+                    </Box>
+                  </Flex>
+                </CardHeader>
+
+                <CardBody overflowY="auto" p={6}>
+                  <VStack align="stretch" gap={6}>
+                    <Box>
+                      <Text fontSize="sm" fontWeight="medium" mb={2}>
+                        Workers
+                      </Text>
+                        
+
+                        <WorkerMultiSelect
+                        value={selectedWorkers}
+                        onChange={setSelectedWorkers}
+                        />
+                    </Box>
+
+                    <Box>
+                      <Text fontSize="sm" fontWeight="medium" mb={2}>
+                        Project
+                      </Text>
+
+                      <ProjectSelect
+                        value={selectedProjectId}
+                        onChange={(v) => setSelectedProjectId(v)}
+                        isRequired={true}
+                        label="Project"
+                        includeNoneOption={false}
+                      />
+                    </Box>
+
+                    <Box>
+                      <Text fontSize="sm" fontWeight="medium" mb={2}>
+                        Worker Message
+                      </Text>
+                      <Box
+                        bg="gray.50"
+                        borderRadius="md"
+                        p={3}
+                        fontSize="sm"
+                        whiteSpace="pre-wrap"
+                      >
+                        {report.report}
+                      </Box>
+                    </Box>
+                  </VStack>
+                </CardBody>
+
+                <CardFooter borderTop="1px solid #eee" p={4}>
+                  <Button variant="ghost" width="100%" onClick={onClose}>
+                    Close
+                  </Button>
+                </CardFooter>
+              </Card.Root>
+
+              {/* RIGHT COLUMN */}
+              <Card.Root width="60%" borderRadius={0}>
+                <CardHeader borderBottom="1px solid #eee">
+                  <Text fontSize="md" fontWeight="bold">
+                    Jobs
+                  </Text>
+                </CardHeader>
+
+                <CardBody overflowY="auto" p={4}>
+                  <Table.Root size="sm" variant="outline">
+                    <Table.Header bg="gray.50" position="sticky" top={0}>
+                      <Table.Row>
+                        <Table.Cell fontWeight="bold" width="20%">
+                          Stage
+                        </Table.Cell>
+                        <Table.Cell fontWeight="bold" width="35%">
+                          Job Type
+                        </Table.Cell>
+                        <Table.Cell fontWeight="bold" width="25%">
+                          Volume
+                        </Table.Cell>
+                        <Table.Cell fontWeight="bold" width="20%">
+                          Actions
+                        </Table.Cell>
+                      </Table.Row>
+                    </Table.Header>
+
+                    <Table.Body>
+                      <Table.Row bg="blue.50">
+                        <Table.Cell>
+                          <Input
+                            size="sm"
+                            value={newJob.stage}
+                            onChange={(e) =>
+                              handleNewJobChange("stage", e.target.value)
+                            }
+                          />
+                        </Table.Cell>
+
+                        <Table.Cell>
+                          <JobsTypeSelectStandalone
+                            value={newJob.jobs_type || undefined}
+                            onChange={handleJobsTypeChange}
+                          />
+                        </Table.Cell>
+
+                        <Table.Cell>
+                          <InputGroup endAddon={currentAddon}>
+                            <Input
+                              size="sm"
+                              value={newJob.total_volume ?? ""}
+                              onChange={(e) =>
+                                handleNewJobChange(
+                                  "total_volume",
+                                  Number(e.target.value)
+                                )
+                              }
+                              placeholder="0.0"
+                            />
+                          </InputGroup>
+                        </Table.Cell>
+
+                        <Table.Cell>
+                          <Button
+                            size="sm"
+                            colorScheme="blue"
+                            onClick={handleAddJobRow}
+                            disabled={!newJob.jobs_type || !newJob.total_volume}
+                          >
+                            Add
+                          </Button>
+                        </Table.Cell>
+                      </Table.Row>
+
+                      {jobs.map((job, i) => (
+                        <Table.Row key={i}>
+                          <Table.Cell>
+                            <Badge>{job.stage}</Badge>
+                          </Table.Cell>
+
+                          <Table.Cell>
+                            {job.jobs_type?.title}
+                          </Table.Cell>
+
+                          <Table.Cell>
+                            {job.total_volume}{" "}
+                            <Text as="span" fontSize="xs" color="gray.500">
+                              {job.jobs_type?.measurement_unit}
+                            </Text>
+                          </Table.Cell>
