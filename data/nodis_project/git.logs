+def compute_progress_by_jobs_type(session, jt_id: str, project_id: str):
+    jobs = session.exec(select(Job).where(and_(Job.project_id == project_id, Job.jobs_type_id == jt_id)))
+    completed_volume = sum(float(j.volume) for j in jobs)
+    estimated_jobs = session.exec(select(JobsEstimation).where(
+        and_(
+            JobsEstimation.project_id == project_id, 
+            JobsEstimation.jobs_type_id == jt_id)
+            )
+        )
+    estimated_volume = sum(float(est.total_volume) for est in estimated_jobs)
+    if completed_volume > estimated_volume:
+        return (1, completed_volume - estimated_volume)
+    return (completed_volume / estimated_volume if estimated_volume > 0 else 0, 0)
