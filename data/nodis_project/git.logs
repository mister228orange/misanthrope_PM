+import ulid
+from typing import Optional
+from decimal import Decimal
+from sqlmodel import SQLModel, Field, Relationship
+from app.core.models import TimestampMixin, ActiveMixin
+from app.api.workers.models import Worker
+
+
+# ----- Inventory Type -----
+class InventoryTypeBase(SQLModel):
+    category: str
+    title: str
+    cost: Decimal = Field(max_digits=10, decimal_places=2)
+
+
+class InventoryTypeCreate(InventoryTypeBase):
+    pass
+
+
+class InventoryTypeUpdate(SQLModel):
+    category: Optional[str] = None
+    title: Optional[str] = None
+    cost: Optional[Decimal] = None
+
+
+class InventoryType(InventoryTypeBase, table=True):
+    id: str = Field(default_factory=ulid.ulid, primary_key=True)
+
+
+class InventoryTypePublic(InventoryTypeBase):
+    id: str
+
+
+class InventoryTypesPublic(SQLModel):
+    data: list[InventoryTypePublic]
+    count: int
+
+class InventoryBase(SQLModel):
+    type_id: str
+    income: Optional[str] = None  # could also use datetime.date
+    is_active: bool = True
+    owner: Optional[str] = Field(default=None, foreign_key="worker.id")
+    serial_number: Optional[str] = None
+
+
+class InventoryCreate(InventoryBase):
+    pass
+
+
+class InventoryUpdate(SQLModel):
+    type: Optional[int] = None
+    income: Optional[str] = None
+    is_active: Optional[bool] = None
+    owner: Optional[str] = None
+    serial_number: Optional[str] = None
+
+
+class Inventory(InventoryBase, TimestampMixin, ActiveMixin, table=True):
+    id: str = Field(default_factory=ulid.ulid, primary_key=True)
+    worker: Optional[Worker] = Relationship(back_populates="inventory")
+
+
+class InventoryPublic(InventoryBase):
+    id: str
+
+
+class InventoriesPublic(SQLModel):
+    data: list[InventoryPublic]
+    count: int
+
+
+
+# ----- Inventory Actions -----
+class InventoryActionBase(SQLModel):
+    tool_id: str = Field(foreign_key="inventory.id")
+    action_date: Optional[str] = None
+    from_worker: Optional[str] = Field(default=None, foreign_key="worker.id")
+    to_worker: Optional[str] = Field(default=None, foreign_key="worker.id")
+
+
+class InventoryActionCreate(InventoryActionBase):
+    pass
+
+
+class InventoryActionUpdate(SQLModel):
+    tool_id: Optional[str] = None
+    action_date: Optional[str] = None
+    from_worker: Optional[str] = None
+    to_worker: Optional[str] = None
+
+
+class InventoryAction(InventoryActionBase, table=True):
+    id: str = Field(default_factory=ulid.ulid, primary_key=True)
+
+
+class InventoryActionPublic(InventoryActionBase):
+    id: str
+
+
+class InventoryActionsPublic(SQLModel):
+    data: list[InventoryActionPublic]
+    count: int
