@@ -0,0 +1,52 @@
+from decimal import Decimal
+from app.models import ProjectPublicDetailed, JobsEstimation
+from typing import Optional
+from sqlmodel import Session, select, and_
+
+from api.projects.crud import get_project_jobs_types
+
+
+def get_current_job_cost(project: ProjectPublicDetailed, jobs_type_id: str) -> Optional[Decimal]:
+    # TODO set in mind to specifications may different costs for each job type.
+    # completed jobs will close specifications jobs estimations by adding order in that way we can find current jobs_estimation what we need.
+    
+    if project.jobs_estimations:
+        for je in project.jobs_estimations:
+            if je.jobs_type.id == jobs_type_id:
+                return je.job_cost_per_unit      
+
+
+def compute_progress_by_jobs_type(session, jt_id: str, project_id: str) -> tuple[float, float]:
+    jobs = session.exec(select(Job).where(and_(Job.project_id == project_id, Job.jobs_type_id == jt_id))) # type: ignore
+    completed_volume = sum(float(j.volume) for j in jobs)
+    estimated_jobs = session.exec(select(JobsEstimation).where(
+        and_(
+            JobsEstimation.project_id == project_id, 
+            JobsEstimation.jobs_type_id == jt_id)
+            )
+        )
+    estimated_volume = sum(float(est.total_volume) for est in estimated_jobs)
+    if completed_volume > estimated_volume:
+        return (1, completed_volume - estimated_volume)
+    return (completed_volume / estimated_volume if estimated_volume > 0 else 0, 0)
+
+
+def compute_progress(project: ProjectPublicDetailed, session: Session):
+        jobs_types = get_project_jobs_types(project)
+        current_total_progress, total_overhead = 0, {}
+        if jobs_types:
+            for jt in jobs_types:
+                complete_part, overhead  = compute_progress_by_jobs_type(session, jt.id, project.id)
+                
+                current_total_progress += complete_part / len(jobs_types)
+                total_overhead[jt] = overhead 
+
+        project.current_progress = current_total_progress
+        project.state = {
+            "current_progress": {
+                "current_total_progress": current_total_progress
+                }, 
+                "total_overhead": total_overhead
+            }
+
+
