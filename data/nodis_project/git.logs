+        –ù–æ–≤—ã—Ö –æ—Ç—á–µ—Ç–æ–≤ ({unacceptedReports.length})
+      </Heading>
+
+      {unacceptedReports.length === 0 ? (
+        <Box textAlign="center" py={8}>
+          <Text color="gray.500">–ù–µ—Ç –Ω–æ–≤—ã—Ö –æ—Ç—á–µ—Ç–æ–≤</Text>
+        </Box>
+      ) : (
+        <Stack gap={4}>
+          {unacceptedReports.map((report: ReportPublic) => (
+            <Card.Root key={report.id} variant="outline">
+              <Card.Header pb={2}>
+                <Flex justify="space-between" align="center">
+                  <Heading size="md">–û—Ç—á–µ—Ç –æ—Ç —Ä–∞–±–æ—Ç–Ω–∏–∫–∞ —Å tg_id: {report.sender_tg_id}</Heading>
+                  <Text fontSize="sm" color="gray.500">ID: {report.id}</Text>
+                </Flex>
+              </Card.Header>
+              <Card.Body>
+                <Stack gap={3}>
+                  <Box>
+                    <Text fontWeight="medium">–û—Ç—á–µ—Ç:</Text>
+                    <Text mt={1} p={3} bg="gray.50" borderRadius="md" whiteSpace="pre-wrap">
+                      {report.report}
+                    </Text>
+                  </Box>
+
