+    session.add(db_pricing)
+    session.commit()
+    session.refresh(db_pricing)
+    return db_pricing
+
+
+def get_project_pricings(session: Session, skip: int = 0, limit: int = 100) -> List[ProjectPricing]:
+    statement = select(ProjectPricing).offset(skip).limit(limit)
+    return list(session.exec(statement))
