+def get_specification_sources_detailed(session: Session, project_id: Optional[str] = None) -> List[SpecificationSource]:
+    statement = (
+        select(SpecificationSource)
+        .options(
+            selectinload(SpecificationSource.jobs_estimations)  # type: ignore
+                .joinedload(JobsEstimation.jobs_type),  # type: ignore
+            joinedload(SpecificationSource.project),  # type: ignore
+        )
+    )
+    
+    if project_id:
+        statement = statement.where(SpecificationSource.project_id == project_id)
+    
+    statement = statement.order_by(SpecificationSource.created_at.desc())  # type: ignore
+    
+    results = session.exec(statement)
+    return list(results.all())
