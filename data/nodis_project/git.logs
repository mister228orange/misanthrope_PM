+    },
+    onSettled: () => {
+      queryClient.invalidateQueries({ queryKey: ["inventories"] })
+    },
+  })
+
+  const onSubmit = async () => {
+    mutation.mutate(inventoryId)
+  }
+
+  return (
+    <DialogRoot
+      size={{ base: "xs", md: "md" }}
