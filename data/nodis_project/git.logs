+def bounty_distribute(job_state: JobBountyState, session: Session):
+    bounty_volume = compute_bounty_state(job_state, session)
+    if bounty_volume:
+        workers_team = get_workers_team(job_state.workers_ids)
+        for worker in workers_team:
+            worker.current_mined_salary += Decimal(bounty_volume * float(worker.productivity_rate) / len(workers_team))
+            session.add(worker)
+        session.commit()
+
+
+def compute_bounty(job: Job) -> Optional[float]:
+    if job.root_report and job.root_report.project:
+        jobs_type_cost = job.root_report.project.get_current_job_cost(job.jobs_type.id)
+        if jobs_type_cost and job.bounties_status != 'actual':
+            return float(job.volume) * float(jobs_type_cost)
+    return None
+
+
+def compute_bounty_state(job_state: JobBountyState, session: Session) -> Optional[float]:
+    if job_state.project_id:
+        project = get_project_detailed(session, job_state.project_id)
+        if project:
+            jobs_type_cost = get_current_job_cost(project, job_state.jobs_type_id)
+            if jobs_type_cost and job_state.volume:
+                return job_state.volume * float(jobs_type_cost)
+    return None
+
