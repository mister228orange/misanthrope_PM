@@ -0,0 +1,73 @@
+from sqlmodel import Session, select
+from typing import Optional
+from .models import (
+    Inventory, InventoryCreate, InventoryUpdate,
+    InventoryType, InventoryTypeCreate, InventoryTypeUpdate,
+    InventoryAction, InventoryActionCreate
+)
+
+
+# -------- INVENTORY --------
+def create_inventory(session: Session, inv_in: InventoryCreate) -> Inventory:
+    db_inv = Inventory.model_validate(inv_in)
+    session.add(db_inv)
+    session.commit()
+    session.refresh(db_inv)
+    return db_inv
+
+
+def get_inventory(session: Session, inv_id: str) -> Optional[Inventory]:
+    return session.get(Inventory, inv_id)
+
+
+def get_inventories(session: Session, skip=0, limit=100) -> list[Inventory]:
+    statement = select(Inventory).offset(skip).limit(limit)
+    return list(session.exec(statement))
+
+
+def update_inventory(session: Session, db_inv: Inventory, inv_in: InventoryUpdate) -> Inventory:
+    update_data = inv_in.model_dump(exclude_unset=True)
+    db_inv.sqlmodel_update(update_data)
+    session.add(db_inv)
+    session.commit()
+    session.refresh(db_inv)
+    return db_inv
+
+
+def delete_inventory(session: Session, inv_id: str):
+    db_inv = get_inventory(session, inv_id)
+    if db_inv:
+        session.delete(db_inv)
+        session.commit()
+
+
+# -------- INVENTORY TYPE --------
+def create_inventory_type(session: Session, type_in: InventoryTypeCreate) -> InventoryType:
+    db_type = InventoryType.model_validate(type_in)
+    session.add(db_type)
+    session.commit()
+    session.refresh(db_type)
+    return db_type
+
+
+def get_inventory_types(session: Session):
+    return list(session.exec(select(InventoryType)))
+
+
+def update_inventory_type(session: Session, db_type: InventoryType, type_in: InventoryTypeUpdate):
+    update_data = type_in.model_dump(exclude_unset=True)
+    db_type.sqlmodel_update(update_data)
+    session.add(db_type)
+    session.commit()
+    session.refresh(db_type)
+    return db_type
+
+
+# -------- INVENTORY ACTIONS --------
+def create_inventory_action(session: Session, action_in: InventoryActionCreate) -> InventoryAction:
+    db_action = InventoryAction.model_validate(action_in)
+    session.add(db_action)
+    session.commit()
+    session.refresh(db_action)
+    return db_action
+
