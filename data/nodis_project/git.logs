+
+@router.get("/", response_model=JobsTypeCategoryTreesPublic, dependencies=[Depends(get_current_user)])
+def list_jobs_type_category_trees(session: SessionDep):
+    """
+    Return all uploaded XML trees with metadata.
+    """
+    trees = session.exec(select(JobsTypeCategoryTree)).all()
+    if trees is None:
+        raise HTTPException(status_code=404, detail="No trees found")
+    return JobsTypeCategoryTreesPublic(data=[JobsTypeCategoryTreePublic.model_validate(tree) for tree in trees], count=len(trees))
+
+
+# ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
+# READ SINGLE TREE
