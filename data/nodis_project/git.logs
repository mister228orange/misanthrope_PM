+from sqlmodel import Session, select
+from .models import Worker, WorkerCreate, WorkerUpdate
+from typing import Optional
+
+
+def create_worker(session: Session, worker_in: WorkerCreate) -> Worker:
+    worker_in = WorkerCreate.model_validate(worker_in)
+    db_worker = Worker(
+        full_name=worker_in.full_name,
+        tg_id=worker_in.tg_id,
+        fixed_salary=worker_in.fixed_salary,
+        productivity_rate=worker_in.productivity_rate
+    )
+    session.add(db_worker)
+    session.commit()
+    session.refresh(db_worker)
+    return db_worker
+
+def get_worker(session: Session, worker_id: int) -> Optional[Worker]:
+    return session.get(Worker, worker_id)
+
+def get_workers(session: Session, skip: int = 0, limit: int = 100) -> list[Worker]:
+    statement = select(Worker).offset(skip).limit(limit)
+    return list(session.exec(statement))
+
