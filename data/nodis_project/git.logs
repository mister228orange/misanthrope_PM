+import { Button } from "@chakra-ui/react"
+import { useMutation, useQueryClient, UseMutationResult } from "@tanstack/react-query"
+import { FaUndo } from "react-icons/fa"
+
+import {
+  InventoryService,
+  type InventoryReinstateWorkerEndpointResponse,
+  type InventoryReinstateWorkerEndpointData
+} from "@/client"
+import useCustomToast from "@/hooks/useCustomToast"
+import { handleError } from "@/utils"
+import { ApiError } from "@/client/core/ApiError"
+
+interface ReinstateInventoryProps {
+  inventoryId: string
+  onSuccess?: () => void
+}
+
+export default function ReinstateInventory({ inventoryId, onSuccess }: ReinstateInventoryProps) {
+  const queryClient = useQueryClient()
+  const { showSuccessToast } = useCustomToast()
+
+  const mutation: UseMutationResult<
+    InventoryReinstateWorkerEndpointResponse,
+    ApiError,
+    InventoryReinstateWorkerEndpointData
+  > = useMutation({
+    mutationFn: (data) => InventoryService.reinstateWorkerEndpoint(data),
+    onSuccess: () => {
+      showSuccessToast("–ò–Ω–≤–µ–Ω—Ç–∞—Ä—å —É—Å–ø–µ—à–Ω–æ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω.")
+      queryClient.invalidateQueries({ queryKey: ["inventory"] })
+      onSuccess?.()
+    },
+    onError: (err) => {
+      handleError(err as ApiError)
+    },
+  })
+
+  return (
+    <Button
+      variant="ghost"
+      size="sm"
+      colorPalette="green"
+      type="button"
+      onClick={() => mutation.mutate({ invId: inventoryId })}
+      loading={mutation.isPending}
+    >
+      <FaUndo /> –í–æ—Å—Å—Ç–∞–Ω–æ–≤–∏—Ç—å
+    </Button>
+  )
+}
