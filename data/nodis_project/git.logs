+from typing import Optional, List, Tuple
+from sqlmodel import Session, select, func
+from app.api.progress.models import (
+    Report, ReportCreate, ReportUpdate,
+    Material, MaterialCreate, MaterialUpdate,
+    MaterialSpend, MaterialSpendCreate, MaterialSpendUpdate,
+    Job, JobCreate, JobUpdate,
+)
+
+
+# ----- Reports -----
+def get_reports(session: Session, skip: int = 0, limit: int = 100) -> Tuple[List[Report], int]:
+    query = select(Report).offset(skip).limit(limit)
+    data = list(session.exec(query).all())
+    total = select(func.count(Report.id)) # ignore
+    return data, total
+
+
+def get_report(session: Session, report_id: str) -> Optional[Report]:
+    return session.get(Report, report_id)
+
+
+def create_report(session: Session, report_in: ReportCreate) -> Report:
+    report = Report.model_validate(report_in)
+    session.add(report)
+    session.commit()
+    session.refresh(report)
+    return report
+
+
+def update_report(session: Session, report: Report, report_in: ReportUpdate) -> Report:
+    update_data = report_in.model_dump(exclude_unset=True)
+    for key, value in update_data.items():
+        setattr(report, key, value)
+    session.add(report)
+    session.commit()
+    session.refresh(report)
+    return report
+
+
+def delete_report(session: Session, report: Report) -> None:
+    session.delete(report)
+    session.commit()
+
+
+# ----- Materials -----
+def get_materials(session: Session, skip: int = 0, limit: int = 100) -> Tuple[List[Material], int]:
+    query = select(Material).offset(skip).limit(limit)
+    data = session.exec(query).all()
+    total = session.exec(select(Material)).count()
+    return data, total
+
+
+def get_material(session: Session, material_id: str) -> Optional[Material]:
+    return session.get(Material, material_id)
+
+
+def create_material(session: Session, material_in: MaterialCreate) -> Material:
+    material = Material.model_validate(material_in)
+    session.add(material)
+    session.commit()
+    session.refresh(material)
+    return material
+
+
+def update_material(session: Session, material: Material, material_in: MaterialUpdate) -> Material:
+    update_data = material_in.model_dump(exclude_unset=True)
+    for key, value in update_data.items():
+        setattr(material, key, value)
+    session.add(material)
+    session.commit()
+    session.refresh(material)
+    return material
+
+
+def deactivate_material(session: Session, material: Material) -> Material:
+    material.deactivate()
+    session.add(material)
+    session.commit()
+    session.refresh(material)
+    return material
+
+
+def delete_material(session: Session, material: Material) -> None:
+    session.delete(material)
+    session.commit()
+
+
+# ----- Material Spends -----
+def get_material_spends(session: Session, skip: int = 0, limit: int = 100) -> Tuple[List[MaterialSpend], int]:
+    query = select(MaterialSpend).offset(skip).limit(limit)
+    data = session.exec(query).all()
+    total = session.exec(select(MaterialSpend)).count()
+    return data, total
+
+
+def get_material_spend(session: Session, spend_id: str) -> Optional[MaterialSpend]:
+    return session.get(MaterialSpend, spend_id)
+
+
+def create_material_spend(session: Session, spend_in: MaterialSpendCreate) -> MaterialSpend:
+    spend = MaterialSpend.model_validate(spend_in)
+    session.add(spend)
+    session.commit()
+    session.refresh(spend)
+    return spend
+
+
+def update_material_spend(session: Session, spend: MaterialSpend, spend_in: MaterialSpendUpdate) -> MaterialSpend:
+    update_data = spend_in.model_dump(exclude_unset=True)
+    for key, value in update_data.items():
+        setattr(spend, key, value)
+    session.add(spend)
+    session.commit()
+    session.refresh(spend)
+    return spend
+
+
+def delete_material_spend(session: Session, spend: MaterialSpend) -> None:
+    session.delete(spend)
+    session.commit()
+
+
+# ----- Jobs -----
+def get_jobs(session: Session, skip: int = 0, limit: int = 100) -> Tuple[List[Job], int]:
+    query = select(Job).offset(skip).limit(limit)
+    jobs = session.exec(query).all()
+    total = session.exec(select(Job)).count()
+    return jobs, total
+
+
+def get_job(session: Session, job_id: str) -> Optional[Job]:
+    return session.get(Job, job_id)
+
+
+def create_job(session: Session, job_in: JobCreate) -> Job:
+    job = Job.model_validate(job_in)
+    session.add(job)
+    session.commit()
+    session.refresh(job)
+    return job
+
+
+def update_job(session: Session, job: Job, job_in: JobUpdate) -> Job:
+    update_data = job_in.model_dump(exclude_unset=True)
+    for key, value in update_data.items():
+        setattr(job, key, value)
+    session.add(job)
+    session.commit()
+    session.refresh(job)
+    return job
+
+
+def delete_job(session: Session, job: Job) -> None:
+    session.delete(job)
