+import { useEffect, useState } from "react"
+import { IconButton } from "@chakra-ui/react"
+import { BsThreeDotsVertical } from "react-icons/bs"
+import { MenuRoot, MenuTrigger, MenuContent } from "@/components/ui/menu"
+import useAuth from "@/hooks/useAuth"
+import EditJobsType from "./EditJobsType"
+import DeleteJobsType from "./DeleteJobsType"
+import ReinstateJobsType from "./ReinstateJobsType"
+import DestroyJobsType from "./DestroyJobsType"
+import { type JobsTypeDetailed } from "@/client"
+
+interface JobsTypeActionsMenuProps {
+  job: JobsTypeDetailed
+  onUpdate?: (updated: JobsTypeDetailed) => void
+}
+
+export const JobsTypeActionsMenu = ({
+  job,
+  onUpdate,
+}: JobsTypeActionsMenuProps) => {
+  const { user } = useAuth()
+  const [isActive, setIsActive] = useState(job.is_active)
+
+  useEffect(() => {
+    setIsActive(job.is_active)
+  }, [job.is_active])
+
+  const handleDelete = () => {
+    setIsActive(false)
+    onUpdate?.({ ...job, is_active: false })
+  }
+
+  const handleReinstate = () => {
+    setIsActive(true)
+    onUpdate?.({ ...job, is_active: true })
+  }
+
+  const handleEditSuccess = (updated: JobsTypeDetailed) => {
+    onUpdate?.(updated)
+  }
+
+  return (
+    <MenuRoot>
+      <MenuTrigger asChild>
+        <IconButton variant="ghost" color="inherit" size="sm">
+          <BsThreeDotsVertical />
+        </IconButton>
+      </MenuTrigger>
+
+      <MenuContent>
+        <EditJobsType
+          job={job}
+          onSuccess={handleEditSuccess}
+        />
+        {isActive ? (
+          <DeleteJobsType
+            jobId={job.id}
+            onSuccess={handleDelete}
+          />
+        ) : (
+          <ReinstateJobsType
+            jobId={job.id}
+            onSuccess={handleReinstate}
+          />
+        )}
+        {user?.is_superuser && (
+          <DestroyJobsType jobId={job.id} />
+        )}
+      </MenuContent>
+    </MenuRoot>
+  )
+}
