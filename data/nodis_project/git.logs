+
+interface SpecificationDetailedProps {
+  sourceId: string
+  onClose: () => void
+}
+
+interface EditCell {
+  id: string
+  field: "total_volume" | "job_price_per_unit" | "material_price_per_unit" | "new"
+}
+
+export const SpecificationDetailed: FC<SpecificationDetailedProps> = ({ sourceId, onClose }) => {
+  const [spec, setSpec] = useState<SpecificationSourcePublic | null>(null)
+  const [estimations, setEstimations] = useState<JobsEstimationPublic[]>([])
+  const [title, setTitle] = useState("")
+  const [editCell, setEditCell] = useState<EditCell | null>(null)
+  const [isEditingTitle, setIsEditingTitle] = useState(false)
+  const [cellValue, setCellValue] = useState("")
+  const [newRow, setNewRow] = useState<{
+    stage: string
+    jobs_type_id: string
+    total_volume?: string
+    job_price_per_unit?: string
+    material_price_per_unit?: string
+    }>({ stage: "", jobs_type_id: "" })
+
+  useEffect(() => {
+    const fetchSpec = async () => {
+      const data = await ProjectsService.readSpecificationSource({ sourceId })
+      setSpec(data)
+      setTitle(data.title)
+    }
+
