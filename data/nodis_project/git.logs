+
+interface SpecificationCardProps {
+  specification: SpecificationSourcePublic
+  onClick?: () => void
+}
+
+export const SpecificationCard: FC<SpecificationCardProps> = ({ 
+  specification, 
+  onClick 
+}) => {
+  const borderColor = "gray.200"
+  const shadow = "md"
+  
+  // Format the updated_at timestamp to a readable date
+  const formattedDate = specification.updated_at 
+    ? new Date(specification.updated_at).toLocaleDateString()
+    : 'No date'
+
+  return (
+    <Box 
+      w="220px" 
+      h="160px" 
+      borderWidth="1px"
+      borderColor={borderColor} 
+      borderRadius="md" 
+      boxShadow={shadow} 
+      p={3} 
+      cursor="pointer" 
+      bg="white" 
+      _hover={{ transform: "scale(1.03)" }} 
+      transition="all 0.2s" 
+      position="relative" 
+      onClick={onClick}
+    >
+      <HStack align="start" gap={4} h="100%">
+        <VStack align="start" flex={1}>
+          <Text fontWeight="bold" fontSize="md">
+            {specification.title}
+          </Text>
+          <Text fontSize="sm" color="gray.600">
+            {formattedDate}
+          </Text>
+        </VStack>
+        
+        <VStack align="start" gap={1}>
+          <Text fontWeight="semibold" fontSize="sm">–†–∞–±–æ—Ç</Text>
+          {/* Add other labels if needed */}
+        </VStack>
+        
+        <VStack align="start" gap={1}>
+          <Text fontWeight="bold" fontSize="sm">
+            {specification.jobs_amount}
+          </Text>
+          {/* Add other values if needed */}
+        </VStack>
+      </HStack>
+      
+      {/* Document link indicator */}
+      {specification.document_link && (
+        <Box position="absolute" bottom={2} right={2}>
+          <Text fontSize="xs" color="blue.500">üìé</Text>
+        </Box>
+      )}
+    </Box>
+  )
+}
