+    title: Optional[str] = None
+    category: Optional[str] = None
+    build_object_id: Optional[str] = None
+    status: Optional[str] = None
+    is_active: Optional[bool] = None
+
+
+class Project(ProjectBase, table=True):
+    __tablename__ = "projects" # type: ignore
+    id: str = Field(default_factory=ulid.ulid, primary_key=True)
+
+
+class ProjectPublic(ProjectBase):
+    id: str
+
+
+class ProjectsPublic(SQLModel):
+    data: List[ProjectPublic]
+    count: int
+
+
+# ----- Jobs Estimation -----
+class JobsEstimationBase(SQLModel):
+    project_id: str = Field(foreign_key="projects.id")
+    jobs_type_id: str = Field(foreign_key="jobs_type.id")
+    total_volume: Optional[Decimal] = Field(default=None, max_digits=10, decimal_places=2)
+    deadline: Optional[date] = None
+
+
+class JobsEstimationCreate(JobsEstimationBase):
+    pass
+
+
+class JobsEstimationUpdate(SQLModel):
+    project_id: Optional[str] = None
+    jobs_type_id: Optional[str] = None
+    total_volume: Optional[Decimal] = None
+    deadline: Optional[date] = None
+
+
+class JobsEstimation(JobsEstimationBase, table=True):
+    __tablename__ = "jobs_estimation" # type: ignore
+    id: str = Field(default_factory=ulid.ulid, primary_key=True)
+
+
+class JobsEstimationPublic(JobsEstimationBase):
+    id: str
+
+
+class JobsEstimationsPublic(SQLModel):
+    data: List[JobsEstimationPublic]
+    count: int
+
+
+# ----- Project Pricing -----
+class ProjectPricingBase(SQLModel):
+    project_id: str = Field(foreign_key="projects.id")
+    jobs_type_id: str = Field(foreign_key="jobs_type.id")
+    job_price_per_unit: Optional[Decimal] = Field(default=None, max_digits=10, decimal_places=2)
+    job_cost_per_unit: Optional[Decimal] = Field(default=None, max_digits=10, decimal_places=2)
+    material_price_per_unit: Optional[Decimal] = Field(default=None, max_digits=10, decimal_places=2)
+    material_cost_per_unit_estimation: Optional[Decimal] = Field(default=None, max_digits=10, decimal_places=2)
+
+
+class ProjectPricingCreate(ProjectPricingBase):
+    pass
+
+
+class ProjectPricingUpdate(SQLModel):
+    project_id: Optional[str] = None
+    jobs_type_id: Optional[str] = None
+    job_price_per_unit: Optional[Decimal] = None
+    job_cost_per_unit: Optional[Decimal] = None
+    material_price_per_unit: Optional[Decimal] = None
+    material_cost_per_unit_estimation: Optional[Decimal] = None
+
+
+class ProjectPricing(ProjectPricingBase, table=True):
+    __tablename__ = "project_pricing" # type: ignore
+    id: str = Field(default_factory=ulid.ulid, primary_key=True)
+
+
+class ProjectPricingPublic(ProjectPricingBase):
+    id: str
+
+
+class ProjectPricingsPublic(SQLModel):
+    data: List[ProjectPricingPublic]
+    count: int
