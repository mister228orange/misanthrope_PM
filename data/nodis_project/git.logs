+from fastapi import APIRouter, Depends, HTTPException, Query
+from sqlmodel import Session
+from typing import List
+from app.api.deps import SessionDep, get_current_user, get_current_active_superuser
+from .models import Project, ProjectCreate, ProjectUpdate, ProjectPublic, JobsEstimation, JobsEstimationCreate, JobsEstimationUpdate, JobsEstimationPublic, ProjectPricing, ProjectPricingCreate, ProjectPricingUpdate, ProjectPricingPublic
+from .crud import create_project, get_project, get_projects, update_project, delete_project, create_jobs_estimation, get_jobs_estimations, update_jobs_estimation, delete_jobs_estimation, create_project_pricing, get_project_pricings, update_project_pricing, delete_project_pricing
+
+router = APIRouter(prefix="/projects", tags=["projects"])
+
+# -------- PROJECT --------
+
+@router.post("/", response_model=ProjectPublic, dependencies=[Depends(get_current_user)])
+def create_project_endpoint(project_in: ProjectCreate, session: SessionDep):
+    return create_project(session, project_in)
+
+
+@router.get("/{project_id}", response_model=ProjectPublic, dependencies=[Depends(get_current_user)])
+def read_project(project_id: str, session: SessionDep):
+    db_project = get_project(session, project_id)
+    if not db_project:
+        raise HTTPException(status_code=404, detail="Project not found")
+    return db_project
+
+
+@router.get("/", response_model=List[ProjectPublic], dependencies=[Depends(get_current_user)])
+def read_projects(session: SessionDep, skip: int = Query(0, ge=0), limit: int = Query(100, ge=1, le=1000)):
+    projects = get_projects(session, skip=skip, limit=limit)
+    return projects
+
+
+@router.put("/{project_id}", response_model=ProjectPublic, dependencies=[Depends(get_current_user)])
+def update_project_endpoint(project_id: str, project_in: ProjectUpdate, session: SessionDep):
+    db_project = get_project(session, project_id)
+    if not db_project:
+        raise HTTPException(status_code=404, detail="Project not found")
+    return update_project(session, db_project, project_in)
+
+
+@router.delete("/{project_id}", dependencies=[Depends(get_current_active_superuser)])
+def delete_project_endpoint(project_id: str, session: SessionDep):
+    db_project = get_project(session, project_id)
+    if not db_project:
+        raise HTTPException(status_code=404, detail="Project not found")
+    delete_project(session, project_id)
+    return {"ok": True}
+
+
+# -------- JOBS ESTIMATION --------
+@router.post("/jobs-estimations/", response_model=JobsEstimationPublic, dependencies=[Depends(get_current_user)])
+def create_jobs_estimation_endpoint(estimation_in: JobsEstimationCreate, session: SessionDep):
+    return create_jobs_estimation(session, estimation_in)
+
+
+@router.get("/jobs-estimations/{estimation_id}", response_model=JobsEstimationPublic, dependencies=[Depends(get_current_user)])
+def read_jobs_estimation(estimation_id: str, session: SessionDep):
+    db_estimation = session.get(JobsEstimation, estimation_id)
+    if not db_estimation:
+        raise HTTPException(status_code=404, detail="Jobs estimation not found")
+    return db_estimation
+
+
+@router.get("/jobs-estimations/", response_model=List[JobsEstimationPublic], dependencies=[Depends(get_current_user)])
+def read_jobs_estimations(session: SessionDep, skip: int = Query(0, ge=0), limit: int = Query(100, ge=1, le=1000)):
+    estimations = get_jobs_estimations(session, skip=skip, limit=limit)
+    return estimations
+
+
+@router.put("/jobs-estimations/{estimation_id}", response_model=JobsEstimationPublic, dependencies=[Depends(get_current_user)])
+def update_jobs_estimation_endpoint(estimation_id: str, estimation_in: JobsEstimationUpdate, session: SessionDep):
+    db_estimation = session.get(JobsEstimation, estimation_id)
+    if not db_estimation:
+        raise HTTPException(status_code=404, detail="Jobs estimation not found")
+    return update_jobs_estimation(session, db_estimation, estimation_in)
+
+
+@router.delete("/jobs-estimations/{estimation_id}", dependencies=[Depends(get_current_active_superuser)])
+def delete_jobs_estimation_endpoint(estimation_id: str, session: SessionDep):
+    db_estimation = session.get(JobsEstimation, estimation_id)
+    if not db_estimation:
+        raise HTTPException(status_code=404, detail="Jobs estimation not found")
+    delete_jobs_estimation(session, estimation_id)
+    return {"ok": True}
+
+# -------- PROJECT PRICING --------
+@router.post("/project-pricings/", response_model=ProjectPricingPublic, dependencies=[Depends(get_current_user)])
+def create_project_pricing_endpoint(pricing_in: ProjectPricingCreate, session: SessionDep):
+    return create_project_pricing(session, pricing_in)
+
+
+@router.get("/project-pricings/{pricing_id}", response_model=ProjectPricingPublic, dependencies=[Depends(get_current_user)])
+def read_project_pricing(pricing_id: str, session: SessionDep):
+    db_pricing = session.get(ProjectPricing, pricing_id)
+    if not db_pricing:
+        raise HTTPException(status_code=404, detail="Project pricing not found")
+    return db_pricing
+
+
+@router.get("/project-pricings/", response_model=List[ProjectPricingPublic], dependencies=[Depends(get_current_user)])
+def read_project_pricings(session: SessionDep, skip: int = Query(0, ge=0), limit: int = Query(100, ge=1, le=1000)):
+    pricings = get_project_pricings(session, skip=skip, limit=limit)
+    return pricings
+
+
+@router.put("/project-pricings/{pricing_id}", response_model=ProjectPricingPublic, dependencies=[Depends(get_current_user)])
+def update_project_pricing_endpoint(pricing_id: str, pricing_in: ProjectPricingUpdate, session: SessionDep):
+    db_pricing = session.get(ProjectPricing, pricing_id)
+    if not db_pricing:
+        raise HTTPException(status_code=404, detail="Project pricing not found")
+    return update_project_pricing(session, db_pricing, pricing_in)
