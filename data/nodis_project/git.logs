+import aiohttp
+from typing import Optional
+from config import cfg
+from .models import ReportCreate, User, InviteToken, Message
+from sqlalchemy.orm import Session
+from infrastructure.db import get_session
+
+
+class BackendMedium:
+
+    def __init__(self, cfg):
+        proto = "https"
+        if cfg.environment == 'local':
+            proto = "http"
+        self.backend_base_url = f"{proto}://backend:8000"
+    
+    async def proceed_request(self, method, endpoint, data=None, headers=None):
+        async with aiohttp.ClientSession() as session:
+            async with session.request(method=method, url=self.backend_base_url+endpoint, json=data) as response:
+                return await response.json()
+    
+    async def send_report(self, report: ReportCreate):
+        response = await self.proceed_request("POST", "/workersreports/simple/submit/", {report})
+        if response.get('status') == 'ok':
+            return True
+        else:
+            raise Exception(response)
+
