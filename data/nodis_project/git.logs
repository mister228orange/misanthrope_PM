new file mode 100644
index 0000000..15ff26e
--- /dev/null
+++ b/frontend/src/components/Inventory/InventoryTimeline.tsx
@@ -0,0 +1,63 @@
+import { Timeline, Text } from "@chakra-ui/react"
+import { LuUser } from "react-icons/lu"
+import { formatUnixTime } from "@/utils"
+import { InventoryActionPublic, WorkerPublic } from "@/client"
+import { useMemo } from "react"
+
+
+type HolderInterval = {
+  holder: WorkerPublic | null
+  start: number
+  end: number | null
+}
+
+function buildInventoryTimeline(actions: InventoryActionPublic[]): HolderInterval[] {
+  if (!actions?.length) return []
+
+  // Sort by time ascending
+  const sorted = [...actions].sort((a, b) => a.action_date??0 - (b.action_date??0))
+
+  return sorted.map((a, i) => ({
+    holder: a.holder ?? null,
+    start: a.action_date??0,
+    end: sorted[i + 1]?.action_date ?? 0,
+  }))
+}
+
+
+export function InventoryTimeline({ actions }: { actions: InventoryActionPublic[] }) {
+  const intervals = useMemo(() => buildInventoryTimeline(actions), [actions])
