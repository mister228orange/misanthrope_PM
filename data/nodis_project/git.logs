diff --git a/frontend/src/components/Common/Combobox.tsx b/frontend/src/components/Common/Combobox.tsx
new file mode 100644
index 0000000..d4e0b56
--- /dev/null
+++ b/frontend/src/components/Common/Combobox.tsx
@@ -0,0 +1,102 @@
+import { useState, useRef, useMemo } from "react"
+import { Button, Popover, Portal, Listbox, Input, useListCollection, useFilter, useListbox } from "@chakra-ui/react"
+import { useQuery } from "@tanstack/react-query"
+import { NomenclatureService, type JobsTypePublic } from "@/client"
+
+export function JobsTypeCombobox({
+  value,
+  onChange,
+  placeholder = "–í—ã–±–µ—Ä–∏—Ç–µ —Ç–∏–ø —Ä–∞–±–æ—Ç—ã",
+}: {
+  value: string
+  onChange: (v: string) => void
+  placeholder?: string
+}) {
+  const triggerRef = useRef<HTMLButtonElement | null>(null)
+  const [inputValue, setInputValue] = useState("")
+  const [open, setOpen] = useState(false)
+
+  const { data, isLoading } = useQuery({
+    queryKey: ["jobsTypes"],
+    queryFn: () => NomenclatureService.readJobsTypes({ onlyActive: true }),
+  })
+
+  type Item = { label: string; value: string }
+
+    
+  const items = useMemo(
+    () =>
+      data?.map((j: JobsTypePublic) => ({
+        label: j.title,
+        value: j.id,
+      })) ?? [],
+    [data]
+  )
+
+    const { contains } = useFilter({ sensitivity: "base" })
+   const { collection, filter } = useListCollection<Item>({
+  initialItems: items,
+  filter: contains,
+})
+  
+   const listbox = useListbox({
+    collection,
+    onValueChange() {
+      setOpen(false)
+      setInputValueFn("")
+      triggerRef.current?.focus()
+    },
+  })
+  const setInputValueFn = (value: string) => {
+    setInputValue(value)
+    filter(value)
+  }
+  const selectedItem = collection.items.find((i) => i.value === value)
+
+  return (
+    <Popover.Root open={open} onOpenChange={(e) => setOpen(e.open)}>
+      <Popover.Trigger asChild>
+        <Button
+          ref={triggerRef}
+          variant="outline"
+          size="sm"
+        >
+          {selectedItem ? selectedItem.label : placeholder}
+        </Button>
+      </Popover.Trigger>
+
+      <Portal>
+        <Popover.Positioner>
+          <Popover.Content _closed={{ animation: "none" }}>
+            <Popover.Body p={0}>
+              <Listbox.RootProvider
+                value={listbox}
+                gap={0}
+              >
+                <Listbox.Input
+                  value={inputValue}
+                  placeholder="Type to search..."
+                  onChange={(e) => setInputValueFn(e.currentTarget.value)}
+                  px={3}
+                  py={2}
+                  roundedTop="l2"
+                  borderBottomWidth={1}
+                  borderColor="gray.200"
+                />
+
+                <Listbox.Content>
+                  {collection.items.map((item: Item) => (
+                    <Listbox.Item item={item} key={item.value}>
+                      <Listbox.ItemText>{item.label}</Listbox.ItemText>
+                      <Listbox.ItemIndicator />
+                    </Listbox.Item>
+                  ))}
+                </Listbox.Content>
+              </Listbox.RootProvider>
+            </Popover.Body>
+          </Popover.Content>
+        </Popover.Positioner>
+      </Portal>
+    </Popover.Root>
+  )
+}
