+
+@router.get("/{tree_id}", response_model=JobsTypeCategoryTreePublic, dependencies=[Depends(get_current_user)])
+def get_jobs_type_category_tree(tree_id: str, session: SessionDep):
+    """
+    Get a single tree by its ID.
+    """
+    tree = JobsTypeCategoryTree.get_by_id(session, tree_id)
+    if not tree:
+        raise HTTPException(status_code=404, detail="Tree not found")
+    return tree
+
+
+# ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
+# DELETE TREE
+# ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ‚îÄ
+
+@router.delete("/{tree_id}", dependencies=[Depends(get_current_active_superuser)])
+def delete_jobs_type_category_tree(tree_id: str, session: SessionDep):
+    """
+    Permanently delete a stored XML tree.
+    """
+    tree = JobsTypeCategoryTree.get_by_id(session, tree_id)
+    if not tree:
+        raise HTTPException(status_code=404, detail="Tree not found")
+
+    JobsTypeCategoryTree.unsafe_delete_by_id(session, tree_id)
+    return {"ok": True}
