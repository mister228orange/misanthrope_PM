+    session.refresh(db_estimation)
+    return db_estimation
+
+
+def get_jobs_estimations(session: Session, skip: int = 0, limit: int = 100) -> List[JobsEstimation]:
+    statement = select(JobsEstimation).offset(skip).limit(limit)
+    return list(session.exec(statement))
+
+
+def update_jobs_estimation(session: Session, db_estimation: JobsEstimation, estimation_in: JobsEstimationUpdate) -> JobsEstimation:
+    update_data = estimation_in.model_dump(exclude_unset=True)
+    db_estimation.sqlmodel_update(update_data)
+    session.add(db_estimation)
+    session.commit()
+    session.refresh(db_estimation)
+    return db_estimation
+
+
+def delete_jobs_estimation(session: Session, estimation_id: str):
+    db_estimation = session.get(JobsEstimation, estimation_id)
+    if db_estimation:
+        session.delete(db_estimation)
+        session.commit()
+
+
+# -------- PROJECT PRICING --------
+def create_project_pricing(session: Session, pricing_in: ProjectPricingCreate) -> ProjectPricing:
