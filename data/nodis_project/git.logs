diff --git a/scripts/generate_client_settings.py b/scripts/generate_client_settings.py
new file mode 100644
index 0000000..4e727e5
--- /dev/null
+++ b/scripts/generate_client_settings.py
@@ -0,0 +1,91 @@
+#!/usr/bin/env python3
+from pathlib import Path
+import secrets
+import sys
+from pathlib import Path
+from jinja2 import Environment, FileSystemLoader
+from dataclasses import dataclass, asdict
+from datetime import datetime
+import json
+
+
+@dataclass
+class ClientEnv:
+    client: str
+    domain: str
+    environment: str
+    project_name: str
+    secret_key: str
+    superuser_email: str
+    superuser_password: str
+    postgres_password: str
+    hashed_password: str
+    generated_at: str
+
+
+def random_string(length: int = 32) -> str:
+    """Generate a secure random URL-safe string."""
+    return secrets.token_urlsafe(length)[:length]
+
+
+def generate_client_env(client: str, domain: str, environment: str = "production") -> ClientEnv:
+    """Create a full client environment configuration object."""
+    return ClientEnv(
+        client=client + ".",
+        domain=domain,
+        environment=environment,
+        project_name=f"{client}-Nodis-Production",
+        secret_key=random_string(50),
+        superuser_email=f"{client}@nodis.com",
+        superuser_password=random_string(16),
+        postgres_password=random_string(40),
+        hashed_password=f"$apr1${random_string(8)}${random_string(22)}",
+        generated_at=datetime.now().isoformat() + "Z",
+    )
+
+
+def main():
+    # === Parse CLI args ===
+    if len(sys.argv) < 3:
+        print("Usage: python generate_env.py <client> <domain>")
+        sys.exit(1)
+
+    client = sys.argv[1]
+    domain = sys.argv[2]
+
+    # === Generate env object ===
+    client_env = generate_client_env(client, domain)
+
+    # === Log what was generated ===
+    print("\nüß© Generated environment object:")
+    print(json.dumps(asdict(client_env), indent=2))
+
+    # === Locate Jinja2 template ===
+    script_dir = Path(__file__).resolve().parent
+    template_path = script_dir.parent / "env_template.j2"
+
+    if not template_path.exists():
+        print(f"\n‚ùå Template not found: {template_path}")
+        sys.exit(1)
+
+    # === Load template ===
+    env = Environment(loader=FileSystemLoader(template_path.parent))
+    template = env.get_template(template_path.name)
+
+    # === Render template ===
+    rendered = template.render(asdict(client_env))
+
+    # === Create client folder ===
+    client_folder_path = script_dir.parent / "clients_settings" / client
+    Path.mkdir(client_folder_path, parents=True, exist_ok=True)
+
+    # === Save result ===
+    output_filename = client_folder_path / f"{client}.env"
+    with open(output_filename, "w") as f:
+        f.write(rendered)
+
+    print(f"\n‚úÖ Environment file generated: {output_filename}")
+
+
+if __name__ == "__main__":
+    main()

