+import { Box } from "@chakra-ui/react";
+import { useEffect, useState } from "react";
+import { JobsTable } from "./JobsTable";
+import { NomenclatureService, JobsTypePublic, ProjectPublicDetailed } from "@/client";
+
+
+
+
+// In your parent component
+export function CurrentProgress({ project }: { project:ProjectPublicDetailed }) {
+  const [jobsTypes, setJobsTypes] = useState<JobsTypePublic[]>([]);
+
+  useEffect(() => {
+    // Fetch jobs types on component mount
+    const fetchJobsTypes = async () => {
+      try {
+        const types = await NomenclatureService.readJobsTypes();
+        setJobsTypes(types);
+      } catch (error) {
+        console.error('Failed to fetch jobs types:', error);
+      }
+    };
+    
+    fetchJobsTypes();
+  }, []);
+
+  return (
+    <Box p={4}>
+      <JobsTable 
+        projectId={project.id}
+        jobsDoned={project.jobs_doned}
+
+      />
+    </Box>
+  );
+}
