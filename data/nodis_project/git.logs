+const fuckingSWAP = (a: Primitive, b: Primitive) => {
+  [a,b] = [b,a]
+}
+
+export const getColorize = ((name: string) => {
+    const customColorSchemes = colorSchemes;
+    const encoder = new TextEncoder();
+    let hash  = encoder.encode(name).reduce((prev, cur) => prev * 256 + cur)
+    while (hash) {
+      let first = hash % 13;
+      hash /= 13;
+      let second = hash % 13;
+      hash /=13;
+      fuckingSWAP(customColorSchemes[first], customColorSchemes[second]);
+    }
+    return customColorSchemes
+})
+
 
 export const getSpecColor = (name: string) => {
   const hash = hashString(name)
   return colorSchemes[hash % colorSchemes.length]
 }
