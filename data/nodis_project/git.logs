+import { Button } from "@chakra-ui/react"
+import { useMutation, useQueryClient, UseMutationResult } from "@tanstack/react-query"
+import { FaUndo } from "react-icons/fa"
+import {
+  WorkersService,
+  type WorkersReinstateWorkerEndpointData,
+  type WorkersReinstateWorkerEndpointResponse,
+} from "@/client"
+import useCustomToast from "@/hooks/useCustomToast"
+import { handleError } from "@/utils"
+import { ApiError } from "@/client/core/ApiError"
+
+interface ReinstateWorkerProps {
+  workerId: string
+  onSuccess?: () => void
+}
+
+export default function ReinstateWorker({ workerId, onSuccess }: ReinstateWorkerProps) {
+  const queryClient = useQueryClient()
+  const { showSuccessToast } = useCustomToast()
+
+  const mutation: UseMutationResult<
+    WorkersReinstateWorkerEndpointResponse,
+    ApiError,
+    WorkersReinstateWorkerEndpointData
+  > = useMutation({
+    mutationFn: (data) => WorkersService.reinstateWorkerEndpoint(data),
+    onSuccess: () => {
+      showSuccessToast("–†–∞–±–æ—Ç–Ω–∏–∫ —É—Å–ø–µ—à–Ω–æ –≤–æ—Å—Å—Ç–∞–Ω–æ–≤–ª–µ–Ω.")
+      queryClient.invalidateQueries({ queryKey: ["workers"] })
+      onSuccess?.()
+    },
+    onError: (err) => {
+      handleError(err as ApiError)
+    },
+  })
+
+  return (
+    <Button
+      variant="solid"
+      colorPalette="green"
+      size="sm"
+      type="button"
+      onClick={() => mutation.mutate({ workerId })}
