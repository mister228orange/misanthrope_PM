@@ -0,0 +1,19 @@
+from app.core.celery import celery_app
+from app.core.db import engine
+from app.api.nomenclature.models import MaterialType
+from sqlmodel import select, Session
+from sqlalchemy.orm import selectinload
+
+
+
+@celery_app.task
+def cleanup_unused_material_types():
+    with Session(engine) as session:
+        diasctivated_mt = session.exec(
+            select(MaterialType).where(MaterialType.is_active==False)
+            .options(selectinload(MaterialType.jobs_types)).all() # type: ignore
+            )
+        for mt in diasctivated_mt:
+            if not mt.jobs_types:
+                session.delete(mt)
+        session.commit()
