@@ -0,0 +1,70 @@
+import { useEffect, useState } from "react"
+import { IconButton } from "@chakra-ui/react"
+import { BsThreeDotsVertical } from "react-icons/bs"
+import { MenuRoot, MenuTrigger, MenuContent } from "@/components/ui/menu"
+import useAuth from "@/hooks/useAuth"
+import EditMaterialType from "./EditMaterialType"
+import DeleteMaterialType from "./DeleteMaterialType"
+import ReinstateMaterialType from "./ReinstateMaterialType"
+import DestroyMaterialType from "./DestroyMaterialType"
+import { type MaterialTypePublic } from "@/client"
+
+interface MaterialTypeActionsMenuProps {
+  materialType: MaterialTypePublic
+  onUpdate?: (updated: MaterialTypePublic) => void
+}
+
+export const MaterialTypeActionsMenu = ({
+  materialType,
+  onUpdate,
+}: MaterialTypeActionsMenuProps) => {
+  const { user } = useAuth()
+  const [isActive, setIsActive] = useState(materialType.is_active)
+
+  useEffect(() => setIsActive(materialType.is_active), [materialType.is_active])
+
+  const handleDelete = () => {
+    setIsActive(false)
+    onUpdate?.({ ...materialType, is_active: false })
+  }
+
+  const handleReinstate = () => {
+    setIsActive(true)
+    onUpdate?.({ ...materialType, is_active: true })
+  }
+
+  const handleEditSuccess = (updated: any) => {
+    onUpdate?.(updated)
+  }
+
+  return (
+    <MenuRoot>
+      <MenuTrigger asChild>
+        <IconButton variant="ghost" color="inherit" size="sm">
+          <BsThreeDotsVertical />
+        </IconButton>
+      </MenuTrigger>
+
+      <MenuContent>
+        <EditMaterialType
+          materialType={materialType}
+          onSuccess={handleEditSuccess}
+        />
+        {isActive ? (
+          <DeleteMaterialType
+            materialTypeId={materialType.id}
+            onSuccess={handleDelete}
+          />
+        ) : (
+          <ReinstateMaterialType
+            materialTypeId={materialType.id}
+            onSuccess={handleReinstate}
+          />
+        )}
+        {user?.is_superuser && (
+          <DestroyMaterialType materialTypeId={materialType.id} />
+        )}
+      </MenuContent>
+    </MenuRoot>
+  )
+}
