+from sqlmodel import Session, select
+from typing import Optional, List
+from .models import Project, ProjectCreate, ProjectUpdate, JobsEstimation, JobsEstimationCreate, JobsEstimationUpdate, ProjectPricing, ProjectPricingCreate, ProjectPricingUpdate
+
+
+# -------- PROJECT --------
+def create_project(session: Session, project_in: ProjectCreate) -> Project:
+    db_project = Project.model_validate(project_in)
+    session.add(db_project)
+    session.commit()
+    session.refresh(db_project)
+    return db_project
+
+
+def get_project(session: Session, project_id: str) -> Optional[Project]:
+    return session.get(Project, project_id)
+
+
+def get_projects(session: Session, skip: int = 0, limit: int = 100) -> List[Project]:
+    statement = select(Project).offset(skip).limit(limit)
+    return list(session.exec(statement))
+
+
+def update_project(session: Session, db_project: Project, project_in: ProjectUpdate) -> Project:
+    update_data = project_in.model_dump(exclude_unset=True)
+    db_project.sqlmodel_update(update_data)
+    session.add(db_project)
+    session.commit()
+    session.refresh(db_project)
+    return db_project
+
+
+def delete_project(session: Session, project_id: str):
+    db_project = get_project(session, project_id)
+    if db_project:
+        session.delete(db_project)
+        session.commit()
+
+
+# -------- JOBS ESTIMATION --------
+def create_jobs_estimation(session: Session, estimation_in: JobsEstimationCreate) -> JobsEstimation:
