+// ‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî
+// Custom NativeSelect wrapper
+// ‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî‚Äî
+import * as React from "react"
+
+type FieldProp = "name" | "value" | "onChange" | "defaultValue"
+
+interface CustomNativeSelectProps
+  extends Omit<React.ComponentProps<typeof NativeSelect.Root>, FieldProp>,
+    Pick<React.ComponentProps<typeof NativeSelect.Field>, FieldProp> {
+  icon?: React.ReactNode
+  items: Array<{ label: string; value: string; disabled?: boolean }>
+}
+
+const CustomNativeSelect = React.forwardRef<
+  HTMLSelectElement,
+  CustomNativeSelectProps
+>(function CustomNativeSelect(props, ref) {
+  const { icon, name, value, defaultValue, onChange, items, children, ...rest } = props
+
+  return (
+    <NativeSelect.Root {...rest}>
+      <NativeSelect.Field
+        ref={ref}
+        name={name}
+        value={value}
+        defaultValue={defaultValue}
+        onChange={onChange}
+      >
+        {children}
+        {items?.map((item) => (
+          <option key={item.value} value={item.value} disabled={item.disabled}>
+            {item.label}
+          </option>
+        ))}
+      </NativeSelect.Field>
+      <NativeSelect.Indicator>{icon}</NativeSelect.Indicator>
+    </NativeSelect.Root>
+  )
+})
+export default CustomNativeSelect
