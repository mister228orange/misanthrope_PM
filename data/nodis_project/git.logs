+import { NativeSelect, Spinner } from "@chakra-ui/react"
+import { useQuery } from "@tanstack/react-query"
+import { WorkersService, type WorkerPublic } from "@/client"
+
+interface WorkerSelectProps {
+  register: any
+  isRequired?: boolean
+  label?: string
+  fieldName?: string
+  defaultValue?: string
+  includeNoneOption?: boolean
+}
+
+export const WorkerSelect = ({
+  register,
+  isRequired = true,
+  label = "–í–ª–∞–¥–µ–ª–µ—Ü",
+  fieldName = "holder",
+  defaultValue,
+}: WorkerSelectProps) => {
+  // –ó–∞–≥—Ä—É–∂–∞–µ–º —Ä–∞–±–æ—Ç–Ω–∏–∫–æ–≤
+  const { data: workers, isLoading } = useQuery({
+    queryKey: ["workers"],
+    queryFn: () => WorkersService.readWorkers({onlyActive: true}
+        
+    ),
+  })
+
+  return (
+      <NativeSelect.Root size="sm" width="280px" disabled={isLoading}>
+        <NativeSelect.Field
+          {...register(fieldName, {
+            required: isRequired ? `${label} –æ–±—è–∑–∞—Ç–µ–ª–µ–Ω` : false,
+          })}
+          defaultValue={defaultValue ?? ""}
+        >
+          <option value="">
+            {isLoading ? "–ó–∞–≥—Ä—É–∑–∫–∞ —Ä–∞–±–æ—Ç–Ω–∏–∫–æ–≤..." : "‚Äî"}
+          </option>
+          {workers?.data?.map((worker: WorkerPublic) => (
+            <option key={worker.id} value={worker.id}>
+              {worker.full_name}
+            </option>
+          ))}
+        </NativeSelect.Field>
+        {isLoading && <Spinner size="sm" />}
+        <NativeSelect.Indicator />
+      </NativeSelect.Root>
+  )
+}
