@@ -0,0 +1,154 @@
+#!/usr/bin/env python3
+"""
+FastAPI Database Migration Reset Script
+This script performs a hard reset of database migrations, backs up data to CSV,
+and creates a single new migration.
+"""
+
+import os
+import subprocess
+import sys
+import pandas as pd
+from sqlalchemy import create_engine, MetaData, Table, inspect
+from sqlalchemy.orm import sessionmaker
+from datetime import datetime
+import shutil
+
+# Get alembic directory from environment variable or ask user
+ALEMBIC_DIR = os.environ.get('ALEMBIC_DIR', "")
+if not ALEMBIC_DIR:
+    ALEMBIC_DIR = input("Please enter the path to your alembic directory: ").strip()
+
+# Configuration - Update these values according to your project
+DATABASE_URL = "your_database_url_here"  # e.g., "postgresql://user:pass@localhost/dbname"
+BACKUP_DIR = os.path.join(ALEMBIC_DIR, "backups")
+MODELS_MODULE = "app.models"  # Where your SQLAlchemy models are defined
+
+def create_backup():
+    """Backup all database tables to CSV files"""
+    print("Creating database backup...")
+    
+    # Create backup directory with timestamp
+    timestamp = datetime.now().strftime("%Y%m%d_%H%M%S")
+    backup_path = os.path.join(BACKUP_DIR, timestamp)
+    os.makedirs(backup_path, exist_ok=True)
+    
+    # Create database engine
+    engine = create_engine(DATABASE_URL)
+    inspector = inspect(engine)
+    
+    # Get all table names
+    table_names = inspector.get_table_names()
+    
+    # Backup each table to CSV
+    for table_name in table_names:
+        try:
+            df = pd.read_sql_table(table_name, engine)
+            backup_file = os.path.join(backup_path, f"{table_name}.csv")
+            df.to_csv(backup_file, index=False)
+            print(f"Backed up {table_name} to {backup_file}")
+        except Exception as e:
+            print(f"Error backing up {table_name}: {e}")
+    
+    return backup_path
+
+def reset_migrations():
+    """Reset all migrations and drop all tables"""
+    print("Resetting migrations...")
+    
+    # Remove all migration files
+    versions_dir = os.path.join(ALEMBIC_DIR, "versions")
+    if os.path.exists(versions_dir):
+        for file in os.listdir(versions_dir):
+            if file != "__init__.py" and file.endswith(".py"):
+                os.remove(os.path.join(versions_dir, file))
+        print("Removed all migration files")
+    
+    # Drop all tables (but not the database itself)
+    try:
+        engine = create_engine(DATABASE_URL)
+        metadata = MetaData()
+        metadata.reflect(bind=engine)
+        metadata.drop_all(engine)
+        print("Dropped all tables")
+    except Exception as e:
+        print(f"Error dropping tables: {e}")
+        return False
+    
+    return True
+
+def create_new_migration():
+    """Create a new initial migration"""
+    print("Creating new migration...")
+    
+    # Change to alembic directory
+    original_dir = os.getcwd()
+    os.chdir(ALEMBIC_DIR)
+    
+    # Generate a new initial migration
+    try:
+        subprocess.run(["alembic", "revision", "--autogenerate", "-m", "initial"], check=True)
+        print("Created new migration")
+    except subprocess.CalledProcessError as e:
+        print(f"Error creating migration: {e}")
+        os.chdir(original_dir)
+        return False
+    
+    # Apply the migration
+    try:
+        subprocess.run(["alembic", "upgrade", "head"], check=True)
+        print("Applied migration")
+    except subprocess.CalledProcessError as e:
+        print(f"Error applying migration: {e}")
+        os.chdir(original_dir)
+        return False
+    
+    # Return to original directory
+    os.chdir(original_dir)
+    return True
+
+def restore_data(backup_path):
+    """Restore data from CSV backup"""
+    print("Restoring data from backup...")
+    
+    engine = create_engine(DATABASE_URL)
+    
+    # Get all CSV files in backup directory
+    csv_files = [f for f in os.listdir(backup_path) if f.endswith('.csv')]
+    
+    for csv_file in csv_files:
+        table_name = csv_file.replace('.csv', '')
+        try:
+            df = pd.read_csv(os.path.join(backup_path, csv_file))
+            df.to_sql(table_name, engine, if_exists='append', index=False)
+            print(f"Restored data to {table_name}")
+        except Exception as e:
+            print(f"Error restoring {table_name}: {e}")
+
+def main():
+    """Main function"""
+    print("Starting database migration reset...")
+    
+    # Confirm action
+    confirm = input("This will reset your database and migrations. Continue? (y/N): ")
+    if confirm.lower() != 'y':
+        print("Operation cancelled")
+        return
+    
+    # Create backup
+    backup_path = create_backup()
+    
+    # Reset migrations and drop tables
+    if reset_migrations():
+        # Create new migration
+        if create_new_migration():
+            # Restore data
+            restore_data(backup_path)
+            print("Migration reset completed successfully!")
+        else:
+            print("Migration creation failed. Your data is backed up at:", backup_path)
+    else:
+        print("Migration reset failed. Your data is backed up at:", backup_path)
+
+if __name__ == "__main__":
+    main()
