+from sqlalchemy.orm import Session
+from .models import User, InviteToken,Message
+from typing import Optional
+from utils import retry
+import secrets
+
+def get_user(session: Session, user_id: int) -> Optional[User]:
+    return session.query(User).filter(User.user_id == user_id).first()
+
+
+def create_message(session: Session, user_id: int, message_type: str, message_text: str) -> None:
+    """Save a message to the database using SQLAlchemy"""
+    message = Message(
+        user_id=user_id,
+        message_type=message_type,
+        message_text=message_text
+    )
+    session.add(message)
+    session.commit()
+
+
+@retry(3)
+def generate_invite_token(session: Session, created_by: Optional[int] = None) -> str:
+    """Generate a new unique invite token using SQLAlchemy with retry logic"""
+    token = secrets.token_urlsafe(16)
+    
+    # Check if token already exists
+    existing_token = session.query(InviteToken).filter(InviteToken.token == token).first()
+    if existing_token:
+        raise Exception("Token already exists")
+    
+    invite_token = InviteToken(
+        token=token,
+        created_by=created_by
+    )
+    session.add(invite_token)
+    session.commit()
+    return token
+
+
+def use_invite_token(session: Session, user_id: int, token: str) -> bool:
+    existing_token = session.query(InviteToken).filter(InviteToken.token == token).first()
+    if not existing_token or existing_token.status != 'active': # type: ignore
+        return False
+    existing_token.used_by = user_id # type: ignore
+    existing_token.status = 'used' # type: ignore
+    session.query(User).filter(User.user_id == user_id).first().is_active = True # type: ignore
+    session.commit()
+    return True
+
+
+def create_user(session: Session, user_id: int, user_info: Optional[dict] = None) ->  User:
+    """Create a new user with SQLAlchemy"""
+    user = User(
+        user_id=user_id,
+        username=user_info.get('username') if user_info else None,
+        first_name=user_info.get('first_name') if user_info else None,
+        last_name=user_info.get('last_name') if user_info else None,
+        state='awaiting_token'
+    )
+    session.add(user)
+    session.commit()
+    return user
