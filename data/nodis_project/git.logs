+export function hexToBlobUrl(hexString?: string | null | undefined, mime = "image/png") {
+  if (!hexString || hexString.length < 2) return "";
+
+  const matched = hexString.match(/.{1,2}/g);
+  if (!matched) return "";
+
+  const bytes = new Uint8Array(matched.map(b => parseInt(b, 16)));
+
+  const blob = new Blob([bytes], { type: mime });
+  return URL.createObjectURL(blob);
 }

