+import { useEffect, useState } from "react"
+import { IconButton } from "@chakra-ui/react"
+import { BsThreeDotsVertical } from "react-icons/bs"
+import { MenuContent, MenuRoot, MenuTrigger } from "../ui/menu"
+
+import type { WorkerPublic } from "../../client"
+import DeleteWorker from "@/components/Workers/DeleteWorker"
+import EditWorker from "./EditWorker"
+import KillWorker from "@/components/Workers/KillWorker"
+import ReinstateWorker from "@/components/Workers/ReinstateWorker"
+import useAuth from "@/hooks/useAuth"
+
+interface WorkerActionsMenuProps {
+  worker: WorkerPublic
+}
+
+export const WorkerActionsMenu = ({ worker }: WorkerActionsMenuProps) => {
+  const { user } = useAuth()
+
+  // –°–∏–Ω—Ö—Ä–æ–Ω–∏–∑–∏—Ä—É–µ–º –ª–æ–∫–∞–ª—å–Ω–æ–µ —Å–æ—Å—Ç–æ—è–Ω–∏–µ —Å –ø—Ä–æ–ø—Å–∞–º–∏
+  const [isActive, setIsActive] = useState(worker.is_active)
+
+  // –û–±–Ω–æ–≤–ª—è–µ–º —Å–æ—Å—Ç–æ—è–Ω–∏–µ –ø—Ä–∏ –∏–∑–º–µ–Ω–µ–Ω–∏–∏ –ø—Ä–æ–ø—Å–∞
+  useEffect(() => {
+    setIsActive(worker.is_active)
+  }, [worker.is_active])
+
+  const handleDelete = () => {
+    setIsActive(false)
+  }
+
+  const handleReinstate = () => {
+    setIsActive(true)
+  }
+
+  return (
+    <MenuRoot>
+      <MenuTrigger asChild>
+        <IconButton variant="ghost" color="inherit">
+          <BsThreeDotsVertical />
+        </IconButton>
+      </MenuTrigger>
+      <MenuContent>
+        <EditWorker worker={worker} />
+        {isActive ? (
+          <DeleteWorker 
+            workerId={worker.id} 
+            onSuccess={handleDelete} // –ü–µ—Ä–µ–¥–∞–µ–º –∫–æ–ª–±—ç–∫
+          />
+        ) : (
+          <ReinstateWorker 
+            workerId={worker.id} 
+            onSuccess={handleReinstate} // –ü–µ—Ä–µ–¥–∞–µ–º –∫–æ–ª–±—ç–∫
+          />
+        )}
+        {user?.is_superuser && <KillWorker workerId={worker.id} />}
+      </MenuContent>
+    </MenuRoot>
+  )
+}
